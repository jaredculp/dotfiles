alias a='ls'
alias aliases="e ~/.dotfiles/aliases"
alias c="cd"
alias cl="clear"
alias code="cd $workspace"
alias e="$EDITOR"
alias la="ls -al"
alias ll="ls -l"
alias ls="ls -G"
alias mci="mvn clean install"
alias mcisc="mvn clean -Dmaven.test.skip.exec=true -Dcheckstyle.skip=true install"
alias mcist="mvn clean -Dmaven.test.skip.exec=true install"
alias mdt="mvn dependency:tree"
alias sl="ls"
alias sp="source ~/.zshrc"
alias u="cd .."
